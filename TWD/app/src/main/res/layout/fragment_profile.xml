<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment_profile"
    android:background="@color/home"
    android:clipToOutline="true"
    android:id="@+id/home">


    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="650dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" >



        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="527dp"
                android:layout_height="735dp"
                android:src="@drawable/group_190"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.504"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="111dp" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="98dp"
                android:layout_height="99dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.116"
                app:srcCompat="@drawable/aa_3" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="340dp"
                android:layout_height="517dp"
                android:src="@drawable/rectangle_312"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="166dp" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.947"
                app:srcCompat="@drawable/group_192" />

            <LinearLayout
                android:layout_width="318dp"
                android:layout_height="355dp"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.494"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_editor_absoluteY="214dp">

                <EditText
                    android:id="@+id/nama_lengkap"
                    android:layout_width="90dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Nama"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/alamat"
                    android:layout_width="92dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Alamat"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/email"
                    android:layout_width="95dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="Email"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/nomor_hp"
                    android:layout_width="101dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="No. Telpon"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/tgl_lahir"
                    android:layout_width="96dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="Tgl Lahir"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/plat_nomor"
                    android:layout_width="107dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="Plat Nomor"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

                <EditText
                    android:id="@+id/nomor_sim"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="No. SIM"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    app:fontFamily="@font/rubik_bold" />

